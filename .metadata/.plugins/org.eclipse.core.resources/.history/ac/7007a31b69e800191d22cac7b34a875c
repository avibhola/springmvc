package com.easy.engineering;
import java.io.*;

public class FileReaderDemo {

	
	public static void main(String[] args) {
		
		File fin=new File("avi.txt");
		FileReader fr=null;
		try{
			fr= new FileReader(fin);
			int ch;
			while((ch=fr.read())!=-1){
				System.out.println(char(ch));
			}
		}
		catch(Exception e){
			
		}
		finally{
			
		}
	}

}
